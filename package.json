{
  "name": "remipicsserver",
  "version": "1.0.0",
  "description": "Remipics server apis back",
  "main": "index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc && npm run prisma:migrate && npm run prisma:seed:user",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npm run prisma:generate && npx prisma migrate dev --skip-generate --skip-seed && npm run prisma:clean",
    "prisma:clean": "ts-node prisma/clean-db.ts",
    "prisma:seed:user": "ts-node ./seeds/users.seeds.ts",
    "test:prisma": "ts-node test/prisma.test.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^9.0.3",
    "@prisma/client": "^5.21.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.7",
    "nodemon": "^3.1.7",
    "prisma": "^5.21.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
